void send_pqt(uint8_t *data, size_t size) {
	uint8_t d;
	for (int i=0; i<size; i++) {
		d = data[i];
		for (int j=0; j<8; j++) {
			digitalWrite(26, d&(1<<7));
			d = d << 1;
			delayMicroseconds(250);
		}
	}
}

void send_data(uint8_t *data) {
	send_pqt(data, 36);
	send_pqt(data, 36);
	send_pqt(data, 36);
	send_pqt(data, 9);
	digitalWrite(26, 0);
}

